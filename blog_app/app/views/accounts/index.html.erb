<p id="notice"><%= notice %></p>

<h1>Accounts#index</h1>

<% @accounts.each do |account| %>
<p>
  <% account.attributes.each do |key, value| %>
    <%= "#{key} = #{value}" %><br>
  <% end %>
  <%= link_to account.first_name, account %><br>
</p>
<% end %>

<%= link_to 'New Account', new_account_path %>
